{
  "name": "flutter-const-optimizer",
  "displayName": "Flutter Const Optimizer",
  "publisher": "PrachiJamdade",
  "description": "Automatically adds the const keyword to Dart/Flutter constructors on save, improving code performance and efficiency.",
  "version": "2.0.0",
  "icon": "images/ext_logo.png",
  "galleryBanner": {
		"color": "#5960a8",
		"theme": "dark"
	},
  "license": "MIT",
  "engines": {
    "vscode": "^1.91.0"
  },
  "keywords": [
		"flutter",
		"dart",
		"const",
		"andriod",
		"ios",
    "optimization"
	],
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Prachi-Jamdade/flutter-const-optimizer-vsext.git"
  },
  "activationEvents": [
    "onLanguage:dart"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutter-const-optimizer.optimizeCode",
        "title": "Optimize Dart Code with Const"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "esbuild": "^0.21.5",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
